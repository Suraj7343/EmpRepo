@model IEnumerable<EmployeeAttendance.BAL.Models.LogInVM>
@{
    ViewBag.Title = "PopUp";
}
<br />
<br />
<div class="panel panel-primary">
    <div class="panel-heading">
        <h1 class="panel-title text-center">TotalTime</h1>
    </div>
    <div class="panel-body">
        <div class="jumbotron">
            <div class="">
                <table class="table text-justify">
                    <tr>
                        <th>ProjectName</th>
                        <th>TimeIn</th>
                        <th>TimeOut</th>
                        <th>TotalTime</th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            @Html.HiddenFor(modelItem => item.Id)
                            <td>
                                @Html.DisplayFor(modelItem => item.ProjectName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TimeIn)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TimeOut)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TotalTime)
                            </td>

                        </tr>
                    }

                </table>
            </div>
        </div>
        <div class="form-group">
            @*<div class="col-md-4">
                  <a class="btn btn-primary" href="/DashBoard/Index">BackToList</a>
                </div>*@
            <div class="col-md-4">
                <a href="/Employee/LogOff?Length=8" class="btn btn-primary">TurnOff</a>
            </div>
        </div>
    </div>
</div>
